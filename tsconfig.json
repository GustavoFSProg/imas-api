{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "rootDir": "./src",
    "baseUrl": "./src",
    // "paths": {},
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src", "Trash/config", "Trash/routes", "Trash/utils", "Trash/controller", "Trash/validators", "Trash/globalVariables.ts"]
}
